{
  "name": "typescript-express-classroom-api",
  "version": "1.0.0",
  "description": "Classroom API server that survives large traffic",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist && rm -rf ./tsconfig.tsbuildinfo",
    "build": "tsc && tsc-alias -f -fe .js",
    "build:clean": "pnpm clean && pnpm build",
    "dev": "pnpm build && node --enable-source-maps dist/index.js",
    "dev:clean": "pnpm clean && pnpm dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "seongbin",
  "license": "ISC",
  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": "24.5.0"
  },
  "dependencies": {
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "tsc-alias": "^1.8.16"
  }
}
